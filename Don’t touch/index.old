    this.link = this.href;
    e.preventDefault();
    if(!this.id) {
      window.location.href = "" + this.pathname;
    } else {
      window.navigator.serviceWorker.register('/sw.js', {
        scope: __uv$config.prefix
      }).then(() => {
        if(!this.id.startsWith("http")){
